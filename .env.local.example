# Example environment variables for local development
# Copy this file to `.env.local` and replace placeholder values before running the app.

# Public (client) keys
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Server secrets â€” DO NOT COMMIT REAL SECRETS
JWT_SECRET=replace-with-a-long-random-secret
BCRYPT_ROUNDS=12
ADMIN_SESSION_DURATION=15m

# Database (only required if USE_DATABASE=true)
USE_DATABASE=false
DB_HOST=localhost
DB_PORT=5432
DB_NAME=dss_atlas
DB_USER=postgres
DB_PASSWORD=postgres

# Static dataset password (for demo endpoints)
STATIC_DATASET_PASSWORD=open123
